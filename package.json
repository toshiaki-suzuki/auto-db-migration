{
  "name": "dbmigration",
  "version": "0.1.0",
  "bin": {
    "dbmigration": "bin/dbmigration.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "export-poetry": "poetry export -f requirements.txt --output lib/lambda-layer/python/requirements.txt --without-hashes",
    "install-python-depemdencies": "pip install -r lib/lambda-layer/python/requirements.txt --target lib/lambda-layer/python/",
    "set-layer": "npm run export-poetry && npm run install-python-depemdencies"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.19",
    "aws-cdk": "^2.172.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-python-alpha": "^2.171.1-alpha.0",
    "aws-cdk-lib": "^2.131.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
